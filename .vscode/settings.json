{
  "hidefiles.configurationType": "local",
  "hidefiles.globalConfig": {
    "profiles": [
      {
        "name": "Default",
        "detail": "",
        "description": "Hide unnecessary files and folders that are almost never used",
        "hidden": [
          ".vscode/",
          ".eslintrc.json",
          ".gitignore",
          "next-env.d.ts",
          "next.config.mjs",
          "package-lock.json",
          "postcss.config.js",
          ".env.local",
          ".next/",
          "node_modules/",
          ".vercel/",
          ".vercel/"
        ]
      },
      {
        "name": "Strict",
        "detail": "Hide everything not immediately required",
        "hidden": [
          "$Default",
          "components/",
          "lib/",
          "public/",
          "database.types.ts",
          "package.json",
          "README.md",
          "tailwind.config.ts",
          "tsconfig.json",
          "vercel.json",
          "app/favicon.ico",
          "app/globals.css"
        ]
      }
    ]
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true
  },
  "hidefiles.selectedProfile": "Show All Files"
}